{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "pattern": "^OK$"
    },
    "code": {
      "type": "integer"
    },
    "total": {
      "type": "integer"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "number": {
            "type": "string"
          },
          "expiration": {
            "type": "string"
          },
          "owner": {
            "type": "string",
            "pattern": "^.{2,}$"
          }
        },
        "required": ["type", "number", "expiration", "owner"]
      }
    }
  },
  "required": ["status", "code", "total", "data"]
}